        "stage":"ResponseComplete",
        "requestURI":"/api/v1/namespaces/default/pods",
        "verb":"create",
        "user":{"username":"admin"},
        "objectRef":{"resource":"pods","namespace":"default","name":"pod-test"},
        "responseStatus":{"code":201}
      }'
